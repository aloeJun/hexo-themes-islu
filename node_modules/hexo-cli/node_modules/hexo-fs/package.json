{
  "name": "hexo-fs",
  "version": "4.1.1",
  "description": "File system module for Hexo.",
  "main": "./dist/fs.js",
  "scripts": {
    "prepublish ": "npm install && npm run clean && npm run build",
    "build": "tsc -b",
    "clean": "tsc -b --clean",
    "eslint": "eslint .",
    "pretest": "npm run clean && npm run build",
    "test": "mocha test/index.js --require ts-node/register",
    "test-cov": "c8 --reporter=lcovonly npm run test"
  },
  "files": [
    "dist/**"
  ],
  "types": "./dist/fs.d.ts",
  "repository": "hexojs/hexo-fs",
  "homepage": "https://hexo.io/",
  "keywords": [
    "file",
    "file system",
    "fs",
    "hexo"
  ],
  "author": "Tommy Chen <tommy351@gmail.com> (https://zespia.tw)",
  "maintainers": [
    "Abner Chou <hi@abnerchou.me> (https://abnerchou.me)"
  ],
  "license": "MIT",
  "dependencies": {
    "bluebird": "^3.7.2",
    "chokidar": "^3.5.3",
    "graceful-fs": "^4.2.10",
    "hexo-util": "^2.7.0"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.36",
    "@types/graceful-fs": "^4.1.5",
    "@types/node": "^18.7.16",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "c8": "^7.12.0",
    "chai": "^4.3.6",
    "eslint": "^8.23.0",
    "eslint-config-hexo": "^5.0.0",
    "eslint-plugin-import": "^2.26.0",
    "mocha": "^10.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3"
  },
  "engines": {
    "node": ">=14"
  }
}
